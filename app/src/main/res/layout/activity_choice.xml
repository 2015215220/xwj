<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#1b8b9b"
    tools:context="bysj.xwj.Choice">


    <Button
        android:id="@+id/btn1_zhuce"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="69dp"
        android:layout_marginLeft="78dp"
        android:layout_marginStart="78dp"
        android:text="正常登陆" />

    <Button
        android:id="@+id/btn1_face"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/btn1_zhuce"
        android:layout_alignBottom="@+id/btn1_zhuce"
        android:layout_marginLeft="52dp"
        android:layout_marginStart="52dp"
        android:layout_toEndOf="@+id/btn1_zhuce"
        android:layout_toRightOf="@+id/btn1_zhuce"
        android:text="人脸识别" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="24dp"
        android:text="@string/login_main_name"
        android:textAlignment="center"
        android:textSize="30sp" />
</RelativeLayout>
